	},
	"homepage": "https://github.com/lasbec/SimpleChords#readme",
	"dependencies": {
		"@pdf-lib/fontkit": "^1.1.1",
		"pdf-lib": "^1.17.1"
	},
	"devDependencies": {
		"@types/node": "^20.8.6",
		"typescript": "^5.2.2",
		"vitest": "^0.34.6"
	}
