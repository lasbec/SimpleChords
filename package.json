{
	"name": "simple-chords",
	"version": "2.4.1",
	"description": "Print your songs for your song book.",
	"main": "./src/Index.js",
	"types": "./type-dist/Index.d.ts",
	"license": "ISC",
	"author": "Lasse Becker",
	"type": "module",
	"scripts": {
		"example": "node ./src/Index.js ./example/Alles.chords.md --log-ast",
